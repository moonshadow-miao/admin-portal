@import './variables.scss';
@import './mixin.scss';
@import './transition.scss';
@import './element-ui.scss';
@import './sidebar.scss';

html, body, #app {
  height: 100%;
}

body {
  -moz-osx-font-smoothing: grayscale;
  -webkit-font-smoothing: antialiased;
  text-rendering: optimizeLegibility;
  font-family: Helvetica, "Microsoft YaHei", "\5FAE\8F6F\96C5\9ED1", STXihei, STHeiti, Heiti, SimSun, sans-serif;
}
@media only screen and (max-width: 768px) {
  body{
    font-size: 12px;
  }
}

* {
  box-sizing: border-box;
}

div:focus {
  outline: none;
}

a:focus,
a:active {
  outline: none;
}

a,
a:focus,
a:hover {
  cursor: pointer;
  color: inherit;
  text-decoration: none;
}

button, input {
  -webkit-appearance: none;
  background-color: #fff;
  outline: none;
  border: 0;
  font-size: 12px
}

.clearfix {
  &:after {
    visibility: hidden;
    display: block;
    font-size: 0;
    content: " ";
    clear: both;
    height: 0;
  }
}

.fl {
  float: left;
}

.fr {
  float: right;
}

//main-container全局样式
.app-main {
  min-height: 100%;
  margin: 0 15px 100px 20px;
  border: 1px solid #e9eced;
  background-color: #fff;
}

#app {

}

.app-container {
  padding: 20px;
}

